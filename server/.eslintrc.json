{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "operator-linebreak": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "comma-dangle": "off",
    "no-tabs": "off",
    "max-len": "off",
    "import/prefer-default-export": "off"
  }
}
